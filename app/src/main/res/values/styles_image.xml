<resources>

    <!--Card General -->
    <style name="imageVectorGeneral">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:tint">@color/primary</item>
        <item name="android:background">@null</item>
    </style>

    <style name="imageVectorLabel" parent="imageVectorGeneral">
        <item name="android:layout_marginStart">@dimen/margin_large</item>
        <item name="android:layout_marginEnd">@dimen/margin_large</item>
        <item name="android:layout_marginTop">@dimen/margin_default</item>
        <item name="android:layout_marginBottom">@dimen/margin_default</item>
    </style>

</resources>
